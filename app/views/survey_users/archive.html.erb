    <td><%=h survey_user.res_kayak %></td>
    <td><%=h survey_user.res_momondo %></td>
    <td><%=h survey_user.res_travelocity %></td>
    <td><%=h survey_user.res_orbitz %></td>
 
    <td><%=h survey_user.resources %></td>
    <td><%=h survey_user.price %></td>
    <td><%=h survey_user.price_long %></td>
    <td width="300px"><%=h survey_user.feedback %></td>
   
  </tr>
<% end %>
</table>

