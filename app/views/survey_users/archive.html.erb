    <td><%=h survey_user.book_travel %></td>
    <td><%=h survey_user.reason_book %></td>
    <td><%=h survey_user.company_policy %></td>
    <td><%=h survey_user.book_same_day %></td>
    <td><%=h survey_user.book_same_day_long %></td>
